# DWC Systems LLC QNIS/PTNI Production Environment Configuration
NODE_ENV=production
PORT=5000

# Application Configuration
APP_NAME=DWC Systems LLC QNIS/PTNI Intelligence Platform
APP_VERSION=1.0.0
APP_DESCRIPTION=Enterprise AI-powered business analytics with quantum-level automation

# Security Configuration
SESSION_SECRET=dwc-qnis-ptni-production-secret-key-ultra-secure-2025
JWT_SECRET=dwc-quantum-intelligence-jwt-secret-2025
ENCRYPTION_KEY=dwc-systems-encryption-key-enterprise-grade

# Database Configuration (will be set by Replit automatically)
# DATABASE_URL=postgresql://...
# PGHOST=...
# PGPORT=...
# PGDATABASE=...
# PGUSER=...
# PGPASSWORD=...

# Performance Settings
MAX_CONCURRENT_CONNECTIONS=1000
CONNECTION_POOL_SIZE=100
CACHE_TTL=3600
REQUEST_TIMEOUT=30000

# Feature Flags
ENABLE_ANALYTICS=true
ENABLE_REALTIME=true
ENABLE_AUTHENTICATION=true
ENABLE_BUSINESS_INTELLIGENCE=true
ENABLE_QUANTUM_MODULES=true

# Business Metrics (Production Values)
TOTAL_LEADS=24
ACTIVE_PROPOSALS=7
PIPELINE_VALUE=485000
SYSTEM_HEALTH_TARGET=98.2
AUTOMATION_LINKAGE=95.6
ROI_PROVEN=156

# AI Module Configuration
AI_MODULES_COUNT=18
WATSON_INTELLIGENCE=true
PIONEX_SYNC=true
AUTOMATION_BINDINGS=6
RUNTIME_KERNEL=true

# Monitoring and Logging
LOG_LEVEL=info
ENABLE_HEALTH_CHECKS=true
ENABLE_PERFORMANCE_MONITORING=true
ENABLE_ERROR_TRACKING=true

# Security Headers
ENABLE_CORS=true
ENABLE_HELMET=true
ENABLE_RATE_LIMITING=true
RATE_LIMIT_WINDOW=900000
RATE_LIMIT_MAX=1000

# Enterprise Settings
ENTERPRISE_MODE=true
MULTI_TENANT=false
