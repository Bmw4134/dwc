{
  "name": "dwc-systems-qnis-ptni-platform",
  "version": "1.0.0",
  "description": "DWC Systems LLC QNIS/PTNI Intelligence Platform - Enterprise AI-powered business analytics with quantum-level automation",
  "type": "web",
  "build": {
    "command": "npm run build",
    "outputDirectory": "dist"
  },
  "start": {
    "command": "npm start"
  },
  "environment": {
    "NODE_ENV": "production",
    "PORT": "5000"
  },
  "health": {
    "path": "/api/health",
    "timeout": 30
  },
  "domains": {
    "primary": "dwc-systems-qnis-ptni.replit.app",
    "custom": null
  },
  "scaling": {
    "instances": 1,
    "memory": "512MB",
    "cpu": "0.5"
  },
  "security": {
    "https": true,
    "headers": {
      "Content-Security-Policy": "default-src 'self' 'unsafe-inline' 'unsafe-eval' data: blob:; frame-ancestors 'self' https://replit.com https://*.replit.com https://*.replit.dev https://*.replit.app",
      "X-Content-Type-Options": "nosniff",
      "X-Frame-Options": "ALLOWALL",
      "Referrer-Policy": "strict-origin-when-cross-origin"
    }
  },
  "features": {
    "authentication": true,
    "realtime": true,
    "analytics": true,
    "mobile_responsive": true,
    "enterprise_grade": true
  },
  "business_metrics": {
    "active_leads": 24,
    "pipeline_value": 485000,
    "system_health": "98.2%",
    "ai_modules": 18,
    "uptime_target": "99.9%"
  },
  "performance": {
    "response_time_target": "< 200ms",
    "concurrent_users": 1000,
    "database_connections": 100,
    "cache_strategy": "redis"
  },
  "monitoring": {
    "health_checks": true,
    "error_tracking": true,
    "performance_monitoring": true,
    "business_intelligence": true
  },
  "deployment_requirements": {
    "node_version": "18+",
    "database": "PostgreSQL",
    "storage": "Persistent",
    "ssl": "Required"
  }
}