{
  "version": 2,
  "builds": [
    {
      "src": "server/index.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server/index.ts"
    },
    {
      "src": "/",
      "dest": "/client/simple-index.html"
    },
    {
      "src": "/login",
      "dest": "/client/login.html"
    },
    {
      "src": "/dashboard",
      "dest": "/server/public/clean-dashboard.html"
    },
    {
      "src": "/(.*)",
      "dest": "/client/simple-index.html"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}